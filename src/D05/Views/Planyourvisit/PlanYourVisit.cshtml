                                            </div>
                                        </div>
                                        <!-- third row ends -->
                                        <!-- fourth row starts -->
                                        <div class="row" id="cart">
                                            <div class="col-md-12">
                                                <div class="panel panel-default panel-info">
                                                    <div class="panel-heading">
                                                        <div class="panel-title">
                                                            <span class="glyphicon glyphicon-shopping-cart"></span>Cart
                                                        </div>
                                                    </div>
                                                    <table class="table table-bordered table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th>Item</th>
                                                                <th>Subtotal</th>
                                                                <th>Edit</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody id="cartitems">
                                                            <tr>
                                                                <td colspan="3">There are no tickets currently added to your cart</td>

                                                            </tr>
                                                            <tr id="totalpricerow"><td colspan="2">Total:</td><td id="cartprice">$0.0</td></tr>

                                                        </tbody>
                                                    </table>

                                                </div>
                                                <div id="next">
                                                    <input type="button" class="btn btn-primary" value="Next" onclick="showFacilities()">
                                                </div>
                                            </div>
                                        </div>
                                        <!-- fourth row ends -->
                                    </div>

                                    <!-- select your ticket ends -->

                                    <div class="tab-pane" id="facilities" style="padding-top: 20px;">
                                        <!-- personal details row-->
                                        <div class="row">
                                            <div class="col-md-12">

                                                <div class="panel panel-success">
                                                    <div class="panel-heading">
                                                        <div class="panel-title">Enter Personal Details</div>
                                                    </div>
                                                    <table class="table">
                                                        <tr>
                                                            <td>First Name <span class="mandatory">*</span>:</td>
                                                            <td><input type="text" id="fn"></td>
                                                            <td>Last Name <span class="mandatory">*</span>:</td>
                                                            <td><input type="text" id="ln"></td>
                                                        </tr>

                                                        <tr>
                                                            <td>Mobile no <span class="mandatory">*</span>:</td>
                                                            <td><input type="text" id="mob"></td>
                                                            <td>Email <span class="mandatory">*</span>:</td>
                                                            <td><input type="text" id="email"></td>
                                                        </tr>
                                                    </table>
                                                </div>
                                                <!-- personal details end-->
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div id="selectFacilities">
                                                            <h4><strong>Additional Facilities</strong></h4>
                                                            <p><strong>Guide Needed</strong> ?&nbsp;<input type="radio" name="guide" value="yes"> Yes</p>
                                                            <p>
                                                                <strong>Reserve Your Meal</strong> : &nbsp;
                                                                <strong>Qty</strong> : <select id="mealqty">
                                                                    <option value="0">0</option>
                                                                    <option value="1">1</option>
                                                                    <option value="2">2</option>
                                                                    <option value="3">3</option>
                                                                    <option value="4">4</option>
                                                                </select>&nbsp;&nbsp;
