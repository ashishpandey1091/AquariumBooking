                                    <div class="tab-pane active" id="type">
                                        <!-- first row -->
                                        <div class="row">

                                            <div class="col-md-12 rowone">
                                                <p><label>Choose Your Date of Visit<span class="mandatory">*</span> : </label>&nbsp;&nbsp;<input type="text" id="datepicker" />
                                            </div>
                                        </div>
                                        <!-- first row ends -->
                                        <!-- second row-->
                                        <div class="row">

                                            <div class="col-md-12">
                                                <div class="panel panel-default panel-info">
                                                    <div class="panel-heading">
                                                        <p class="panel-title">
                                                            Your Entry Ticket
                                                            <!--<span>
                                                            <label id="date">Date of Visit : </label>&nbsp;&nbsp;<input type="text" id="datepicker" />
                                                        </span>-->
                                                        </p>
                                                    </div>
                                                    <table class="table table-bordered table-striped">
                                                        <thead>
                                                            <tr>
                                                                <th>Ticket Type</th>
                                                                <th>Price</th>
                                                                <th>Qty</th>
                                                                <th>Subtotal</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr>
                                                                <td>Adult</td>
                                                                <td>$12.99</td>
                                                                <td>
                                                                    <select class='qty' onchange="entrypassprice('adult',this.value)">
                                                                        <option value="0">0</option>
                                                                        <option value="1">1</option>
                                                                        <option value="2">2</option>
                                                                        <option value="3">3</option>
                                                                    </select>
                                                                </td>
                                                                <td id="subtotalone">--</td>
                                                            </tr>

                                                            <tr>
                                                                <td>Child</td>
                                                                <td>$5.99</td>
                                                                <td>
                                                                    <select class='qty' onchange="entrypassprice('child',this.value)">
                                                                        <option value="0">0</option>
                                                                        <option value="1">1</option>
                                                                        <option value="2">2</option>
                                                                        <option value="3">3</option>
                                                                    </select>
                                                                </td>
                                                                <td id="subtotalthree">--</td>
                                                            </tr>

                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
