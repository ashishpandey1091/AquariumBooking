<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Aquatix</title>

    <environment names="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/home.css" />
        <link rel="stylesheet" href="~/css/planyourvisit.css" />

    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.5/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />

    </environment>
    <style>
        
    </style>
</head>
<body>
    @using System.Security.Claims
    <input type="hidden" value=@ViewData["key"] id="tabInfo">
    <div id="planyourvisit">
        <div class="navbar-wrapper">
            <div class="container">
                <!-- start  of navigation bar-->

                <nav class="navbar navbar-inverse" id="navid">
                    <div class="container">

                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-controls="navbar">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="#">Aquatix Aquarium</a>
                        </div>
                        <div id="navbar" class="navbar-collapse collapse">
                            <ul class="nav navbar-nav">
                                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                                <li class="dropdown">
                                    <a href="html/planyourvisit.html" class="dropdown-toggle"
                                       data-toggle="dropdown" role="button">Visitor Info<b class="caret"></b></a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li><a href="/Planyourvisit/PlanYourVisit?tab=hours">Hours and Prices</a></li>
                                        <li><a href="/Planyourvisit/PlanYourVisit?tab=tickets">Buy Tickets</a></li>
                                        <li><a href="/Planyourvisit/PlanYourVisit?tab=occasions">Special Occasions</a></li>
                                    </ul>
                                </li>

                                <li class="dropdown">
                                    <a asp-controller="" asp-action="" class="dropdown-toggle"
                                       data-toggle="dropdown" role="button">Exhibits<b class="caret"></b></a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li><a href="/Exhibits/Exhibits?tab=exone">Journey to Atlantis</a></li>
                                        <li><a href="/Exhibits/Exhibits?tab=extwo">Dolphin Notch</a></li>
                                        <li><a href="/Exhibits/Exhibits?tab=exthree">Seaworld Tower</a></li>
                                        <li><a href="/Exhibits/Exhibits?tab=exfour">Walrus Skerry</a></li>
                                        <li><a href="/Exhibits/Exhibits?tab=exfive">Killer Whales</a></li>
                                        <li><a href="/Exhibits/Exhibits?tab=exsix">Coral Reef</a></li>
                                        <li><a href="/Exhibits/Exhibits?tab=exseven">Danty Sealions</a></li>

                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a asp-controller="" asp-action="" class="dropdown-toggle"
                                       data-toggle="dropdown" role="button">More Info<b class="caret"></b></a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li><a asp-controller="Home" asp-action="About">About us</a></li>
                                        <li><a asp-controller="Home" asp-action="Contact">Contact us</a></li>
                                        <li><a asp-controller="Home" asp-action="FAQs">FAQs</a></li>
                                        <li><a href="">Webpage1</a></li>
                                        <li><a href="">Webpage2</a></li>
                                        <li><a href="">Webpage3</a></li>

                                    </ul>
                                </li>

                                @if (User.IsSignedIn() && User.IsInRole("admin"))
                                {
                                    <li><a asp-controller="Admin" asp-action="Index">Admin</a></li>
                                }
                            </ul>


                            @if (User.IsSignedIn())
                            {
                                <div id="myaccount" style="padding-right:30px">
                                    <form asp-controller="Account" asp-action="LogOff" method="post" id="logoutForm" class="nav navbar-nav navbar-right">
                                        <ul class="nav navbar-nav">
                                            <li>
                                                <a asp-controller="Manage" asp-action="Index">Hello @User.GetUserName()</a>
                                            </li>
                                            <li>
                                                <button type="submit" class="btn btn-link navbar-btn navbar-link">Log off</button>
                                            </li>
                                        </ul>
                                    </form>
                                </div>
                            }
                            else {
                                <div id="loginlinks" style="padding-right:30px">
                                    <ul class="nav navbar-nav navbar-right">
                                        <li><a asp-controller="Account" asp-action="Register">Register</a></li>
                                        <li><a asp-controller="Account" asp-action="Login">Log in</a></li>
                                    </ul>
                                </div>
                            } 
                        </div>
                    </div>
                </nav>

                <!-- end of navigation bar-->
            </div>
        </div>

        <div class="container" style="margin-top:-10px;">

            <!-- start of breadcrumb -->
            <div class="row">
                <div class="col-md-12">
                    <ul class="breadcrumb">
                        <li><a href="../home.html">Back</a></li>
                        <li><strong>Visitor Info</strong></li>
                    </ul>
                </div>
            </div>
            <!-- end  of breadcrumb -->

            <!-- start of nav tab left -->
            <div class="tabbable tabs-left">
                <div class="row">

                    <!-- start of list for nav tab left-->
                    <div class="col-md-3" id='tabmenu'>
                        <ul class="nav nav-pills nav-stacked">
                            <li id="hourstab"><a href="#hours" data-toggle='tab'>Hours and Prices</a></li>
                            <li id="ticketstab"><a href="#tickets" data-toggle='tab'>Buy Tickets</a></li>
                            @*<li id="findtab"><a href="#find" data-toggle='tab'>Finding us</a></li>*@
                            <li id="occsntab"><a href="#occasions" data-toggle='tab'>Special Occasions</a></li>
                            @*<li id="faqtab"><a href="#faq" data-toggle='tab'>FAQ's</a></li>*@
                        </ul>
                    </div>
                    <!-- end of list for nav tab left-->

                    <!-- start of nav tab content -->
                    <div class="tab-content col-md-9">

                        <!-- start of div content for hours and prices-->
                        <div class="tab-pane active" id="hours">
                            <!-- first row-->
                            <div class="row">
                                <div class="col-md-6">

                                    <div class="panel panel-default panel-info">
                                        <div class="panel-heading">
                                            <h3 class="panel-title">Best Time to Visit</h3>
                                        </div>
                                        <div class="panel-body">
                                            <p class="one">The Aquatix aquarium is open 365 days a year.</p>
                                            <p class="one">
                                                Hours of operation are from <b>10a.m. - 5p.m.</b> daily on normal days and
                                                <b>9:30a.m - 6p.m.</b> on seasonally/statutory holiday weekends.
                                            </p>
                                            <p class="one"><b>Check out timings of our exciting <a href="#">Exhibits!</a></b></p>
                                            <h5>Note:All Exhibits can be visited on an Entry Ticket</h5>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="panel panel-default panel-info">
                                        <div class="panel-heading panel-info">
                                            <h3 class="panel-title">Standard Admission Prices</h3>
                                        </div>
                                        <table class="table table-striped table-bordered">
                                            <thead>
                                                <tr><th>Classification</th><th>Price</th></tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>Adult</td>
                                                    <td>$12.99</td>
                                                </tr>
                                                <tr>
                                                    <td>Child</td>
                                                    <td>$5.9</td>
                                                </tr>
                                                <tr>
                                                    <td>Adult(60yrs and older)</td>
                                                    <td>Entry Free</td>
                                                </tr>
                                                <tr>
                                                    <td>Child(3yrs and below)</td>
                                                    <td>Entry Free</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <!-- first row ends-->
                            <!-- second row-->
                            <div class="row">
                                <div class="col-md-12">
                                    <h3>Important Rules:</h3>
                                    <div>
                                        <p>
                                            During winter hours, the quitest times to visit are on weekdays or prior
                                            to 12 p.m. During summer hours , the quietest times to visit are prior to
                                            11 a.m. or after 4 p.m.
                                        </p>
                                        <h3>Member Hour's</h3>
                                        Members are able to visit the Aquarium unlimited number of times during Regular Hours
                                        of Operation - 10 a.m. - 5 p.m.
                                        During non-holiday weekends(Saturday and Sunday),our doors will open for members
                                        at 9:30 a.m. -30 minutes before we open to the public.
                                        <p id='rulestwo'><b>Not a member?<a href="#"> Join Today</a></b></p>

                                        <p class="rulestwo">No Smoking Policy: Aquatix prohibits Smoking</p>
                                        <p class="rulestwo">Have A Question?</p>
                                        <p>Please call us <strong>(901)8572427</strong> or email us at <strong>visitexperience@aquatix.org</strong></p>

                                    </div>
                                </div>
                            </div>
                            <!-- second row ends-->
                            <!-- third row-->
                            <!--<div class="row">
                            <div class="col-md-12">
                                <div class="panel panel-default">
                                    <div class="panel-body">

                                    </div>
                                    <div class="panel-footer">

                                    </div>
                                </div>
                            </div>
                        </div>-->
                            <!-- third row ends-->
                        </div>
                        <!-- end of div content for hours and prices-->
                        <!-- start of div content for tickets-->
                        <div class="tab-pane" id="tickets">
                            <div class="tabbable">
                                <div class="row">
                                    <div class="col-md-12" id='ticketmenu'>
                                        <ul class="nav nav-tabs">
                                            <li id="typeli" class="active"><a href="#type" data-toggle="tab">Your Ticket</a></li>
                                            <li id="facilitiesli"><a href="#facilities">Additional Facilities</a></li>
                                            <li id="paymentli"><a href="#payment">Payment</a></li>
                                            <li id="confirmli"><a href="#confirm">Complete!</a></li>

                                        </ul>
                                    </div>

                                </div>

                                <div class="tab-content">
