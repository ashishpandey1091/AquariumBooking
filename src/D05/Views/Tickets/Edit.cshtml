@model D05.Models.Ticket

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<form asp-action="Edit">
    <div class="form-horizontal">
        <h4>Ticket</h4>
        <hr />
        <div asp-validation-summary="ValidationSummary.ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="TicketGeneratedDate" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="TicketGeneratedDate" class="form-control" />
                <span asp-validation-for="TicketGeneratedDate" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="TicketType" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="TicketType" class="form-control" />
                <span asp-validation-for="TicketType" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="VisitReason" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="VisitReason" class="form-control" />
                <span asp-validation-for="VisitReason" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="VisitingDate" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="VisitingDate" class="form-control" />
                <span asp-validation-for="VisitingDate" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="VisitingTime" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="VisitingTime" class="form-control" />
                <span asp-validation-for="VisitingTime" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}
